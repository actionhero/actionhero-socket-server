{
  "author": "Evan Tahler <evan@delicioushat.com>",
  "name": "actionhero-socket-server",
  "description": "A TCP and JSON server for actionhero",
  "version": "2.1.1",
  "homepage": "http://www.actionherojs.com",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/actionhero/actionhero-socket-server.git"
  },
  "main": "index.js",
  "keywords": [
    "api",
    "realtime",
    "socket",
    "web",
    "game",
    "cluster",
    "soa",
    "action",
    "service",
    "tcp",
    "json"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@types/jest": "^26.0.15",
    "actionhero": "^24.0.4",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "test": "jest",
    "pretest": "npm run lint && npm run build",
    "lint": "prettier --check src __tests__",
    "dev": "ts-node-dev --no-deps --transpile-only ./src/server.ts",
    "build": "./node_modules/.bin/tsc --declaration && rm -rf dist/actions dist/config",
    "start": "node ./dist/server.js",
    "prepare": "npm run lint && npm run build"
  }
}
